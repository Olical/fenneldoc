;; -*- mode: fennel; -*-
;; Configuration file for Fenneldoc.
;; https://gitlab.com/andreyorst/fenneldoc

{:fennel-path ["cljlib/?.fnl" "src/?.fnl"]
 :function-signatures true
 :insert-comment true
 :insert-copyright true
 :insert-license true
 :insert-version true
 :keys {:copyright "_COPYRIGHT"
        :description "_DESCRIPTION"
        :doc-order "_DOC_ORDER"
        :license "_LICENSE"
        :module-name "_MODULE_NAME"
        :version "_VERSION"}
 :mode "checkdoc"
 :order "aplhabetic"
 :out-dir "./doc"
 :sandbox true
 :test-requirements {}
 :toc true}
